<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafx.LogInController">
   <children>
      <Button fx:id="login" layoutX="229.0" layoutY="261.0" mnemonicParsing="false" onAction="#LogIn" text="LogIn" />
      <PasswordField fx:id="logInPassword" layoutX="226.0" layoutY="202.0" />
      <TextField fx:id="logInUserName" layoutX="226.0" layoutY="146.0" />
      <Label layoutX="226.0" layoutY="121.0" prefHeight="17.0" prefWidth="61.0" text="Username" />
      <Label layoutX="226.0" layoutY="179.0" prefHeight="17.0" prefWidth="61.0" text="Password" />
      <Label fx:id="loginerror" alignment="CENTER" contentDisplay="CENTER" layoutX="138.0" layoutY="326.0" prefHeight="17.0" prefWidth="324.0">
         <font>
            <Font size="25.0" />
         </font></Label>
      <ImageView fx:id="crying" fitHeight="232.0" fitWidth="182.0" layoutX="21.0" layoutY="86.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="rage" fitHeight="232.0" fitWidth="182.0" layoutX="397.0" layoutY="86.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="login1" layoutX="313.0" layoutY="261.0" mnemonicParsing="false" onAction="#logOff" text="Go Back" />
   </children>
</AnchorPane>

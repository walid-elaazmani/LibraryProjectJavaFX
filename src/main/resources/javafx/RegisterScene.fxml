<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafx.RegisterSceneController">
   <children>
      <TextField fx:id="name" layoutX="232.0" layoutY="116.0" promptText="Firstname + lastname" />
      <TextField fx:id="password" layoutX="232.0" layoutY="201.0" promptText="Password" />
      <TextField fx:id="userName" layoutX="232.0" layoutY="159.0" promptText="Username" />
      <Button layoutX="232.0" layoutY="259.0" mnemonicParsing="false" onAction="#register" text="Register" />
      <Button layoutX="321.0" layoutY="259.0" mnemonicParsing="false" onAction="#goBack" text="Go back" />
      <Label fx:id="message" alignment="CENTER" contentDisplay="CENTER" layoutX="232.0" layoutY="324.0" prefHeight="17.0" prefWidth="149.0">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <ImageView fx:id="wrongImage" fitHeight="188.0" fitWidth="187.0" layoutX="27.0" layoutY="107.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
